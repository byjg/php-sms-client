{
  "name": "byjg/sms-client",
  "description": "A generic and extensible lightweight sms client to publish SMS service providers like Twilio and ByJG.",
  "autoload": {
    "psr-4": {
      "ByJG\\SmsClient\\": "src/"
    }
  },
    "autoload-dev": {
        "psr-4": {
        "Tests\\": "tests/"
        }
    },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.1",
    "ext-curl": "*",
    "byjg/webrequest": "^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "vimeo/psalm": "^5.9"
  },
  "license": "MIT"
}
